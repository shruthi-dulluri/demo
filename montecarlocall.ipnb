    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "OsXNted9V131"
      },
      "source": [
        "# **Option prices using python**"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "w6MkQXBJV-z0"
      },
      "source": [
        "import random\r\n",
        "import math"
      ],
      "execution_count": 19,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "uhUyNfq5WDMF"
      },
      "source": [
        "#share specific\r\n",
        "risk_free=0.1\r\n",
        "S_0=100\r\n",
        "sigma =0.3"
      ],
      "execution_count": 32,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "tG3LWt46WQ0m"
      },
      "source": [
        "#option specific\r\n",
        "strike =110\r\n",
        "T=1\r\n",
        "current_time =0"
      ],
      "execution_count": 37,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "R7AmNBs0WaDl"
      },
      "source": [
        "def terminal_shareprice(S_0,risk_free_rate,sigma,Z,T):\r\n",
        "  return S_0*np.exp((risk_free_rate-sigma**2/2)*T+sigma*np.sqrt(T)*Z)\r\n",
        "\r\n"
      ],
      "execution_count": 38,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "kvBCeEBuX3mW"
      },
      "source": [
        "def discounted_callof_pay(S_T,K,risk_free_rate,T):\r\n",
        "  return np.exp(-risk_free_rate*T)*np.maximum(S_T-K,0)"
      ],
      "execution_count": 41,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "JNQyiBsYY76N"
      },
      "source": [
        "np.random.seed(0)\r\n",
        "mcall_estimates = [None]*50\r\n",
        "mcall_std = [None]*50\r\n",
        "for i in range(1,51):\r\n",
        "  norm_array = norm.rvs(size = i*1000)\r\n",
        "  term_call =terminal_shareprice(S_0,risk_free,sigma,norm_array,T-current_time)\r\n",
        "  mcal_val = discounted_callof_pay(term_call,strike,risk_free,T-current_time)\r\n",
        "  mcall_estimates[i-1]= np.mean(mcal_val)\r\n",
        "  mcall_std[i-1] = np.std(mcal_val)/np.sqrt(i*1000)"
      ],
      "execution_count": 42,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "uwA8pHdKaYPc"
      },
      "source": [
        "#code for european vanila call option\r\n",
        "d1=(math.log(S_0/strike)+(risk_free+sigma**2)/(T-current_time))/(sigma*math.sqrt(T-current_time))\r\n",
        "d2 = d1 -sigma*math.sqrt(T-current_time)\r\n",
        "analytic_calloption = S_0*norm.cdf(d1)- strike*math.exp(-risk_free*(T-current_time))*norm.cdf(d2)"
      ],
      "execution_count": 43,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
