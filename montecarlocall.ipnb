        "outputId": "53b4db3a-8954-4a66-c003-23c9803ba984"
      },
      "source": [
        "plt.plot([analytic_calloption]*50)\r\n",
        "plt.plot(mcall_estimates,'.')\r\n",
        "plt.plot(analytic_calloption+np.array(mcall_std)*3,'r')\r\n",
        "plt.plot(analytic_calloption-np.array(mcall_std)*3,'r')\r\n",
        "plt.xlabel('samplesize')\r\n",
        "plt.ylabel('value')"
      ],
