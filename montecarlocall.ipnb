      },
      "source": [
        "#e^x montecarlo simulation\r\n",
        "np.random.seed(0)\r\n",
        "minexp_estimates = [None]*50\r\n",
        "minexp_std = [None]*50\r\n",
        "for i in range(1,51):\r\n",
        "  norm_array = norm.rvs(size = i*1000)\r\n",
        "  exp_value = np.exp(-norm_array**2/2)*np.sqrt(2*np.pi)\r\n",
        "  minexp_estimates[i-1]= np.mean(exp_value)\r\n",
        "  minexp_std[i-1] = np.std(exp_value)/np.sqrt(i*1000)\r\n",
        "# plot of exp\r\n",
        "plt.plot([np.sqrt(np.pi)]*50)\r\n",
        "plt.plot(minexp_estimates,'.')\r\n",
        "plt.plot(np.sqrt(np.pi)+np.array(minexp_std)*3,'r')\r\n",
        "plt.plot(np.sqrt(np.pi)-np.array(minexp_std)*3,'r')\r\n",
        "plt.xlabel('samplesize')\r\n",
        "plt.ylabel('value')"
      ],
      "execution_count": 17,
