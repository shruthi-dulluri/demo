      "source": [
        "import numpy as np\r\n",
        "from scipy.stats import uniform\r\n",
        "from scipy.stats import norm"
      ],
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ZelIWFecJdEi"
      },
      "source": [
        "np.random.seed(0)\r\n",
        "minsin_estimates = [None]*50\r\n",
        "minsin_std = [None]*50\r\n",
        "for i in range(1,51):\r\n",
        "  unif_array = uniform.rvs(size = i*1000)*np.pi\r\n",
        "  sin_val = np.sin(unif_array)*np.pi\r\n",
        "  minsin_estimates[i-1]= np.mean(sin_val)\r\n",
        "  minsin_std[i-1] = np.std(sin_val)/np.sqrt(i*1000)"
      ],
      "execution_count": 5,
      "outputs": []
    },
    {
